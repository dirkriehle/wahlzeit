********************************************************************************************
Collaboration of  Building with BuildingType 
Role types: Client, Building (Base Object), BuildingType (Type Object)
----------------------------------------------------------------------------------------------
public collaboration BaseType {
	
	public role Type {
		
		public BuildingType getSuperType();
		public BuildingType setSuperType(BuildingType sutype);
		public BuildingType getSubType();
		public Set getSubType(BuildingType sutype);
	}
	
	public role Base {
	

		public getBuildingType(Building building);
	}
	
	public role Client {
		
	}
} 

//Bind

public class BuildingType binds BaseType.Type {

}
